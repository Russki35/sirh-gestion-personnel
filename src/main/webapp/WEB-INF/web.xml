<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<display-name>sgp</display-name>
	<servlet>
		<servlet-name>LCC</servlet-name>
		<servlet-class>dev.sgp.web.ListerCollaborateursController</servlet-class>	
	</servlet>
	
	<servlet>
		<servlet-name>CCC</servlet-name>
		<servlet-class>dev.sgp.web.CreerCollaborateurController</servlet-class>	
	</servlet>
	
	<servlet>
		<servlet-name>ECC</servlet-name>
		<servlet-class>dev.sgp.web.EditerCollaborateursController</servlet-class>	
	</servlet>
	
	
	<servlet-mapping>
		<servlet-name>LCC</servlet-name>
		<url-pattern>/collaborateurs/listerCollab</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>CCC</servlet-name>
		<url-pattern>/collaborateurs/creerCollab</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>ECC</servlet-name>
		<url-pattern>/collaborateurs/editer</url-pattern>
	</servlet-mapping>
	


</web-app>